{
  "extends": "./_base.json",
  "compilerOptions": {
    "composite": true,
    "lib": [
      "ESNext",
      "DOM",
      "DOM.Iterable",
      "WebWorker"
    ],
    "jsx": "preserve",
    "jsxImportSource": "vue",
    "paths": {
      "@/*": ["./src/*"],
      "@vitepress/theme/*": ["../../node_modules/vitepress/dist/client/theme-default/*"]
    },
    "tsBuildInfoFile": "../node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "types": [
      "vite/client",
      "vitepress/client"
    ]
  },
  "include": [
    "../.vitepress/**/*",
    "../.vitepress/**/*.json",
    "../.vitepress/**/*.vue",

    "../src/**/*",
    "../src/**/*.json",
    "../src/**/*.vue"
  ]
}
